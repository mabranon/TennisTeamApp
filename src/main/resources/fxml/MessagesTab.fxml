<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_RIGHT" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.applegrocer.message.MessagesTabController">
   <children>
      <TableView fx:id="table" prefHeight="934.0" prefWidth="1587.0">
        <columns>
          <TableColumn fx:id="colFrom" prefWidth="150.0" resizable="false" text="From" />
          <TableColumn fx:id="colSubject" maxWidth="1.7976931348623157E308" prefWidth="1437.0" resizable="false" text="Subject" />
        </columns>
      </TableView>
      <Button fx:id="btnNewMessage" mnemonicParsing="false" onAction="#newMessageAction" text="Write New Message" />
   </children>
   <padding>
      <Insets bottom="20.0" top="20.0" />
   </padding>
</VBox>
